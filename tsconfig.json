{
  "compilerOptions": {
    "moduleResolution": "node",
    "target": "ES2023",
    "lib": [
      "ES2023",
      "dom"
    ],
    "types": [
      "cypress",
      "node"
    ],
    "strict": true,
    "baseUrl": "./cypress",
    "paths": {
      "@pageObject/*": [
        "pageObjects/*"
      ],
      "@enum/*": [
        "support/Enum/*"
      ],
      "@type/*": [
        "support/Types/*"
      ]
    }
  },
  "include": [
    "**/*.ts"
  ],
}